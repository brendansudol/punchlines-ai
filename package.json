    "@upstash/redis": "^1.19.3",
    "@vercel/analytics": "^1.1.2",
    "class-variance-authority": "^0.7.0",
    "classnames": "^2.3.2",
    "clsx": "^2.0.0",
    "leo-profanity": "^1.7.0",
    "lodash.shuffle": "^4.2.0",
    "lucide-react": "^0.303.0",
    "next": "13.4.3",
    "next-themes": "^0.2.1",
    "openai": "^4.1.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hot-toast": "^2.4.1",
