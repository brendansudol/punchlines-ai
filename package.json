    "@types/lodash.shuffle": "^4.2.7",
    "@types/node": "^20.2.5",
    "@types/react": "^18.2.7",
    "@vercel/style-guide": "^5.1.0",
    "autoprefixer": "^10.4.14",
    "encoding": "^0.1.13",
    "eslint": "^8.48.0",
    "eslint-config-next": "13.4.3",
    "eslint-config-prettier": "^8.8.0",
